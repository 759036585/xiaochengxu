.search .weui-search-bar {
  background-color: var(--themeColor);
}
.swiper_box {
  width: 750rpx;
  height: 360rpx;
}
.swiper_box image {
  width: 750rpx;
  height: 360rpx;
}
.daohang {
  display: flex;
}
.daohang .daohang_item {
  padding: 20rpx;
  flex: 1;
}
.floor .floor_group {
  padding: 0 10rpx;
}
.floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.floor .floor_group .floor_title image {
  width: 100%;
}
.floor .floor_group .product_list {
  overflow: hidden;
}
.floor .floor_group .product_list navigator {
  float: left;
  width: 33.33%;
}
.floor .floor_group .product_list navigator:nth-last-child(-n+4) image {
  height: 26.07497845vw;
  border-left: 10rpx solid #fff;
}
.floor .floor_group .product_list navigator:nth-child(2),
.floor .floor_group .product_list navigator:nth-child(3) {
  border-bottom: 5rpx solid #fff;
}
.floor .floor_group .product_list navigator image {
  width: 100%;
  height: 100%;
}
.top {
  position: fixed;
  display: flex;
  width: 90rpx;
  height: 90rpx;
  bottom: 20rpx;
  right: 20rpx;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 50%;
}
.top view {
  color: #fff;
  font-size: 30rpx;
}
